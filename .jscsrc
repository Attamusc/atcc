{
  "preset": "./node_modules/jscs-preset-behance/jscs.json",
  "fileExtensions": [
    ".js",
    ".jsx"
  ],
  "excludeFiles": [
    "assets/**",
    "build/**",
    "node_modules/**"
  ],
  "esprima": "babel-jscs",
  "validateQuoteMarks": {
    "mark": "'",
    "escape": true,
    "ignoreJSX": true
  }
}
